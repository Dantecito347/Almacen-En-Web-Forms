<configuration>
  <system.web>
    <compilation debug="true" targetFramework="4.8">
      <buildProviders>
        <add extension=".cs" type="Microsoft.CSharp.CSharpCodeProvider" />
      </buildProviders>
    </compilation>
    <httpRuntime targetFramework="4.8" />
  </system.web>
  <connectionStrings>
    <add name="AlmacenConnectionString" connectionString="Data Source=localhost\SQLEXPRESS;Initial Catalog=Almacen;Integrated Security=True" providerName="System.Data.SqlClient" />
  </connectionStrings>
  <system.webServer>
    <defaultDocument enabled="true">
      <files>
        <add value="MenuPrincipal.aspx" />
      </files>
    </defaultDocument>
  </system.webServer>
</configuration>